{
  "name": "nuclear",
  "version": "0.0.1",
  "main": "dist/nuclear.js",
  "repository": "https://github.com/reactide-community/reactide.git",
  "author": "Simon Hochrein <simon@ministrycrm.com>",
  "license": "MIT",
  "scripts": {
    "dev-start": "concurrently \"yarn run build\" \"yarn start\"",
    "dev": "parcel src/index.html --target electron",
    "build": "parcel src/index.html --target electron",
    "pkg": "electron-builder",
    "dist": "yarn run build && yarn run pkg",
    "build-main": "parcel watch index.js --target electron",
    "start": "NODE_ENV=development electron ."
  },
  "dependencies": {
    "@types/antd": "^1.0.0",
    "antd": "^3.3.1",
    "babel-polyfill": "^6.26.0",
    "codemirror": "^5.33.0",
    "parcel-bundler": "^1.6.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-select": "^1.2.1",
    "semantic-ui-css": "^2.2.12",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.55",
    "@types/electron": "^1.6.10",
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "electron-builder": "^19.54.0",
    "electron-react-devtools": "^0.5.3",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "less": "^2.7.3",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.2.3",
    "watch": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "build": {
    "appId": "org.reactide-comunity.reactide",
    "directories": {
      "output": "build"
    }
  }
}
